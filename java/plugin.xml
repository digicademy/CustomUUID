<?xml version="1.0" encoding="UTF-8"?>
<!-- The plugin.dtd file is located in the OXYGEN_INSATALL_DIR/plugins directory -->
<!DOCTYPE plugin SYSTEM "../plugin.dtd">

<plugin
 id="net.namenforschung.customUUID"
 name="CustomUUID"
 description="Resolve Custom Editor Variable with UUID"
 version="${project.version}"
 vendor="Namenforschung.net"
 class="net.namenforschung.customUUID.workspace.WorkspaceAccessPlugin"
 classLoaderType="preferReferencedResources">
 <runtime>
        <library name="lib/${project.build.finalName}.jar"/>
 </runtime>
 
 <extension type="WorkspaceAccess" 
  class="net.namenforschung.customUUID.workspace.CustomWorkspaceAccessPluginExtension"/>
</plugin>
